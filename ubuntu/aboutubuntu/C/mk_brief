	cp -r ../../images/C/*.* ../../../build/ubuntu/images/C
		
	cp ../../libs/ubuntu.css ../../../build/ubuntu/about-ubuntu-brief/

	echo "--- Complete. Find outputs at ../../../build/ubuntu/about-ubuntu-brief/"
