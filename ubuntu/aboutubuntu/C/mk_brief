# This is a test make file for the desktop guide

	rm -f ../../../build/ubuntu/about-ubuntu-brief/C/*.*

	echo "--- Building About Ubuntu Document"
	
	xsltproc -o ../../../build/ubuntu/about-ubuntu-brief/C/index.html ../../libs/ubuntu-html-cust.xsl about-ubuntu-brief.xml

