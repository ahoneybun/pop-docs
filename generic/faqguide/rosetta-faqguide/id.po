"stop)\n"
"log_begin_msg \"Stopping system log daemon...\"\n"
"start-stop-daemon --stop --quiet --oknodo --exec $binpath --pidfile "
"$pidfile\n"
"log_end_msg $?\n"
"..."
msgstr ""
"...\n"
"stop)\n"
"log_begin_msg \"Stopping system log daemon...\"\n"
"start-stop-daemon --stop --quiet --oknodo --exec $binpath --pidfile "
