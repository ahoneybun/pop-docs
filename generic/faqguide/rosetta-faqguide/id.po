msgid ""
"\n"
"\t\t\t\t\t\t\t\tsudo mkdir /media/windows \n"
"\t\t\t\t\t\t\t\tsudo mount /dev/hda1 /media/windows/ -t ntfs -o umask=0222"
msgstr ""
"\n"
"\t\t\t\t\t\t\t\tsudo mkdir /media/windows \n"
"\t\t\t\t\t\t\t\tsudo mount /dev/hda1 /media/windows/ -t ntfs -o umask=0222"

#: faqguide.xml:2583(para) faqguide.xml:2619(para)
