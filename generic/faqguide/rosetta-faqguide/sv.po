"option domain-name-servers 202.188.0.133, 202.188.1.5;\n"
"option domain-name \"tm.net.my\";\n"
"option routers 192.168.0.1;\n"
"option broadcast-address 192.168.0.255;\n"
"default-lease-time 600;\n"
"max-lease-time 7200;\n"
"}"
