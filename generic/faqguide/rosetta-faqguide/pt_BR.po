"sudo depmod -a ; sudo modprobe fglrx\n"
"sudo cp /etc/X11/xorg.conf /etc/X11/xorg.conf_backup sudo sed -i -e "
"'s/\"ati\"/\"fglrx\"/' /etc/X11/xorg.conf"

