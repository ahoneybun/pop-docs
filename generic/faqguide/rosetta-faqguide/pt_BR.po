#, no-wrap
msgid ""
"\n"
"\t\t\t\t\t\t\t\tsudo mkdir /media/windows \n"
"\t\t\t\t\t\t\t\tsudo cp /etc/fstab /etc/fstab_backup \n"
"\t\t\t\t\t\t\t\tsudo gedit /etc/fstab"
