"security = user \n"
"username map = /etc/samba/smbusers"

#: faqguide.xml:3446(programlisting)
#, no-wrap
msgid ""
"\n"
"...\n"
"    # By default, the home directories are exported read-only. Change next\n"
"    # parameter to 'yes' if you want to be able to write to them.\n"
"    writable = no\n"
"..."
msgstr ""
"\n"
"...\n"
"    # Por padrão, os diretório do home são exportados com acesso de somente "
"leitura. Altere-o a seguir\n"
"    # parametro para 'yes' caso deseje permitir escritas.\n"
"    writable = no\n"
"..."

#: faqguide.xml:3455(programlisting)
#, no-wrap
msgid ""
"\n"
"# By default, the home directories are exported read-only. Change next\n"
"# parameter to 'yes' if you want to be able to write to them.\n"
"writable = yes"
msgstr ""
"\n"
"# Por padrão, os diretório do home são exportados com acesso de somente "
