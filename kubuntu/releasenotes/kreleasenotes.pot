msgid "Other Changes"
msgstr ""

#: C/releasenotes.xml:463(para) 
msgid "Linux Kernel &linux-kernel-version;"
msgstr ""

#: C/releasenotes.xml:468(para) 
msgid "GCC &gcc-version;"
msgstr ""

#: C/releasenotes.xml:473(para) 
