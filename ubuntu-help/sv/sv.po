msgid "Ubuntu Desktop Guide"
msgstr ""

#: C/index.page:16(title)
msgid ""
"<media type=\"image\" src=\"figures/ubuntu-logo.png\">Ubuntu Logo</media> "
"Ubuntu Desktop Guide"
msgstr ""

#: C/index.page:34(p)
msgid ""
